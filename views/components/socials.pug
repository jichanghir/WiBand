mixin socials(classes)
    div(class=`socials ${classes}`)
        a.socials__link(href="https://www.instagram.com/wibandofficial/" target="_blank")
            svg
                use(xlink:href="/build/images/sprite.svg#instagram-logo")
        a.socials__link(href="https://www.facebook.com/wibandofficial/" target="_blank")
            svg
                use(xlink:href="/build/images/sprite.svg#facebook-logo")
